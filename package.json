{
  "name": "asymptote-helper",
  "publisher": "rayhuang2006",
  "displayName": "Asymptote: Competitive Programming",
  "description": "A competitive programming companion.",
  "version": "0.0.3",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rayhuang2006/asymptote.git"
  },
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cpp",
    "onLanguage:c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "secondarySidebar": [
        {
          "id": "asymptote-view-container",
          "title": "Asymptote",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "asymptote-view-container": [
        {
          "type": "webview",
          "id": "asymptote-sidebar",
          "name": "Asymptote Runner",
          "icon": "media/icon.svg",
          "contextualTitle": "Asymptote"
        }
      ]
    },
    "commands": [
      {
        "command": "asymptote.refreshComplexity",
        "title": "Asymptote: Refresh Complexity"
      },
      {
        "command": "asymptote.openRunner",
        "title": "Open Asymptote Runner",
        "icon": "media/icon.svg"
      },
      {
        "command": "asymptote.toggleCodeLens",
        "title": "Asymptote: Toggle Complexity Lens"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "asymptote.openRunner",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Asymptote",
      "properties": {
        "asymptote.enableCodeLens": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable Asymptote complexity analysis CodeLens."
        },
        "asymptote.chromePath": {
          "type": "string",
          "default": "",
          "description": "Custom path to Google Chrome executable for the parser."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.2.0",
    "chrome-finder": "^1.0.7",
    "puppeteer-core": "^24.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "web-tree-sitter": "^0.20.8"
  }
}