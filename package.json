{
  "name": "asymptote",
  "publisher": "rayhuang",
  "displayName": "asymptote",
  "description": "A competitive programming companion.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cpp",
    "onLanguage:c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "secondarySidebar": [
        {
          "id": "asymptote-view-container",
          "title": "Asymptote",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "asymptote-view-container": [
        {
          "type": "webview",
          "id": "asymptote-sidebar",
          "name": "Asymptote Runner",
          "icon": "media/icon.svg",
          "contextualTitle": "Asymptote"
        }
      ]
    },
    "commands": [
      {
        "command": "asymptote.refreshComplexity",
        "title": "Asymptote: Refresh Complexity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}